{
	"name": "CollegeInsider",
	"author": "Ken Hilton",
	"url": "https://github.com/Kenny2github/CollegeInsiderSkin",
	"description": "Skin for College Insider",
	"type": "skin",
	"ValidSkinNames": {
		"collegeinsider": "CollegeInsider"
	},
	"AutoloadClasses": {
		"SkinCollegeInsider": "CollegeInsider.skin.php",
		"CollegeInsiderTemplate": "CollegeInsiderTemplate.php"
	},
	"MessagesDirs": {
		"CollegeInsider": "i18n"
	},
	"requires": {
		"MediaWiki": ">= 1.36.0"
	},
	"ResourceModules": {
		"skins.collegeinsider": {
			"class": "ResourceLoaderSkinModule",
			"position": "top",
			"features": {
				"content-links": true,
				"content-thumbnails": true,
				"interface-message-box": true,
				"interface-category": true,
				"content-tables": true,
				"i18n-ordered-lists": true,
				"i18n-all-lists-margins": true,
				"i18n-headings": true,
				"elements": true,
				"interface": true,
				"logo": true
			},
			"styles": {
				"resources/css/main.css": {
					"media": "screen"
				},
				"resources/css/min-1019.5.css": {
					"media": "only screen and (min-width: 1019.5px)"
				},
				"resources/css/max-1019.5.css": {
					"media": "only screen and (max-width: 1019.5px)"
				},
				"resources/css/max-800.css": {
					"media": "only screen and (max-width: 800px)"
				},
				"resources/css/max-750.css": {
					"media": "only screen and (max-width: 749.9px)"
				},
				"resources/css/max-666.css": {
					"media": "only screen and (max-width: 666px)"
				},
				"resources/css/max-500.css": {
					"media": "only screen and (max-width: 499.9px)"
				},
				"resources/css/max-360.css": {
					"media": "only screen and (max-width: 360px)"
				},
				"resources/css/min-500.css": {
					"media": "only screen and (min-width: 500px)"
				},
				"resources/css/min-750.css": {
					"media": "only screen and (min-width: 750px)"
				}
			}
		}
	},
	"ResourceFileModulePaths": {
		"localBasePath": "",
		"remoteSkinPath": "CollegeInsider"
	},
	"manifest_version": 1
}